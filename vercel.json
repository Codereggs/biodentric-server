{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ],
  "crons": [
    {
      "path": "/server/server-cron.js",
      "schedule": "*/10 * * * * *"
    }
  ]
}
